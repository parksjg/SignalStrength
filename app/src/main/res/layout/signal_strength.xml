<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".PhoneStateListenerActivity">

    <TextView
        android:id="@+id/signalStrengthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/signalStrengthTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/signalStrengthTextView"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LTE Signal Strength"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= RSRP 2"
        android:id="@+id/textViewRSRP"
        android:layout_marginStart="21dp"
        android:layout_alignTop="@+id/signalStrengthTextView2"
        android:layout_toEndOf="@+id/signalStrengthTextView2" />

    <TextView
        android:id="@+id/cellIDTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textViewRSRP"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= Cell ID"
        android:id="@+id/textViewCellID"
        android:layout_alignTop="@+id/cellIDTextView"
        android:layout_alignStart="@+id/textViewRSRP" />

    <TextView
        android:id="@+id/cellMccTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textViewCellID"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/cellMncTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/cellMccTextView"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/cellPciTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/cellMncTextView"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/cellTacTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/cellPciTextView"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= MCC"
        android:id="@+id/textView2"
        android:layout_alignBottom="@+id/cellMccTextView"
        android:layout_alignStart="@+id/textViewCellID" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= MNC"
        android:id="@+id/textView3"
        android:layout_alignBottom="@+id/cellMncTextView"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= PCI"
        android:id="@+id/textView4"
        android:layout_above="@+id/cellTacTextView"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= TAC"
        android:id="@+id/textView5"
        android:layout_alignBottom="@+id/cellTacTextView"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="= RSRP 1"
        android:id="@+id/textView6"
        android:layout_alignBottom="@+id/signalStrengthTextView"
        android:layout_alignStart="@+id/textViewRSRP" />

</RelativeLayout>
